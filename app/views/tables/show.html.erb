<h2><%= @table.table_name %></h2>
<div><%= link_to 'Edit Table', edit_user_event_table_path(current_user, @event, @table) %></div>
<p>Max Seats: <%= @table.max_seats %></p>
<ul>
	<% @table_attendees.each do |attendee| %>
  <li>
    <tr>
      <td><%= attendee.first_name %> <%= attendee.last_name %></td>
    </tr>
  </li>
  <% end %>
</ul>
<div><%= link_to "Back to Event Details", user_event_path(current_user, @event) %></div>